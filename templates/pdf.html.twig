<html>
<head>
    {% block stylesheets %}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
        <meta charset="UTF-8" >
        <style>
            body {font-size: 13px;font-family:Arial;}
            .card-header {background-color:#244062;text-align:center;}
            .card-body {padding:5px;}
            .table-sm {margin-bottom:0;}
            .table-sm td, .table-sm th {font-size: 13px;padding: 1px 5px;}
            .table-sm thead, .table-sm tfoot {background-color:#dee2e6;}
            .table-sm.no-border td {border:0 none;}
            .list-check-area ul {padding-left:0;}
            .list-check-area li {list-style-type:none;}
            .list-check-area li:before {content:"âœ“";font-weight:bold;padding-right:5px}
            .pdf-cell {background-color:#eaf0f6;color:#000;border:1px solid #000;}
            .pdf-cell-label {color:#000;text-align:right;padding:1mm;margin-bottom:1mm;}
            .pdf-cell-value {background-color:#eaf0f6;text-align:left;color:#000;border:1px solid #000;min-height:26px;padding:2px;margin-bottom:3px;}
        </style>
    {% endblock %}
</head>
<body class="container" style="width:990px;">{# <body class="container" style="width:990px;"> pour un debug rapide #}
    
    <table style="width:100%;">
        <tbody>
            <tr>
                <td style="border:0 none;">
                    <img src='data:image/png;base64,'/>
                </td>
                <td style="border:0 none;">
                    {% block header %}{% endblock %}
                </td>
            </tr>
        </tbody>
    </table>

    <div style="width:100%;height:2px;background-color:#000;"></div>

    {% block body %}
    
    {% endblock %}

